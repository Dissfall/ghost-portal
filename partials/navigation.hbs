{{#if isSecondary}}
        {{#foreach navigation}}
            <li><a href="{{url}}" class="text-white hover:text-gray-200">{{label}}</a></li>
        {{/foreach}}
{{else}}
        {{#foreach navigation}}
            <li><a href="{{url absolute="true"}}" class="text-white hover:text-gray-200">{{label}}</a></li>
        {{/foreach}}
{{/if}}
